<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"  
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- json -->
	<package name="comm_json" namespace="/comm" extends="json-base">
		<action name="searchComm_*" class="searchCommAction" method="{1}">
			<result type="json" />
		</action>
		<action name="treeComm_*" class="treeCommAction" method="{1}">
			<result type="json" />
		</action>
		<action name="rtuBase_*" class="rtuBaseAction" method="{1}">
			<result type="json"/>
		</action>
	</package>
</struts>